some = x -> (
  :show -> (:some, x),
  :bind -> f -> f x
);

none =
  :show -> :none,
  :bind -> f -> none;

pure = x -> some x;

initial_to_name =
  :a -> some :alice,
  :b -> some :bob,
  _ -> none;

choose_winner =
  :alice -> some :alice_wins,
  _ -> none;

pure :a :bind initial_to_name :bind choose_winner :show
